    <pre style="font-size: 12px; word-spacing: 0px; background-color: rgb(230, 230, 230); text-align: -webkit-auto; color: rgb(0, 0, 0); text-indent: 0px; letter-spacing: normal; font-weight: normal; font-variant: normal; font-style: normal; text-transform: none; line-height: 18px">create procedure proc_name (in parameter integer)<br/>begin<br/>declare variable varchar(20);<br/>if parameter=1 then<br/>set variable='MySQL';<br/>else<br/>set variable='PHP';<br/>end if;<br/>insert into tb (name) values (variable);<br/>end;  </pre>
