</node>
<node CREATED="1303096524437" ID="ID_1175441620" MODIFIED="1303096541359" TEXT="~">
<node CREATED="1303096525125" ID="ID_65762029" MODIFIED="1303096536609" TEXT="[def]Bit-wise invert"/>
<node CREATED="1303096543468" ID="ID_799131006" MODIFIED="1303096599968" TEXT="The bit-wise inversion of x is -(x+1)"/>
<node CREATED="1303096602171" ID="ID_1875888158" MODIFIED="1303096613906" TEXT="~5 gives -6"/>
