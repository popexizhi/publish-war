<node CREATED="1362967148592" ID="ID_651573306" MODIFIED="1362967184826">
<richcontent TYPE="NODE"><html>
  <head>
    
  </head>
  <body>
    <p>
      &gt;&gt;&gt; def foo(arg): -----------------------------1
    </p>
    <p>
      ...&#160;&#160;&#160;&#160;&#160;x = 1
    </p>
    <p>
      ...&#160;&#160;&#160;&#160;&#160;print locals()
    </p>
    <p>
      ...&#160;&#160;&#160;&#160;
    </p>
    <p>
      &gt;&gt;&gt; foo(7)--------------------------------------2
    </p>
    <p>
      {'arg': 7, 'x': 1}
    </p>
    <p>
      &gt;&gt;&gt; foo('bar')--------------------------------------3
    </p>
    <p>
      {'arg': 'bar', 'x': 1}
    </p>
