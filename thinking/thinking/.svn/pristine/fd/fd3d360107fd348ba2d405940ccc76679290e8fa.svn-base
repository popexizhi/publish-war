<node CREATED="1329184011328" FOLDED="true" ID="ID_548678397" MODIFIED="1332311857523" TEXT="Example 8.6. Introducing urllister.py">
<node CREATED="1329184304125" ID="ID_875413045" MODIFIED="1329184314968">
<richcontent TYPE="NODE"><html>
  <head>
    
  </head>
  <body>
    <p>
      from sgmllib import SGMLParser
    </p>
    <p>
      class URLLister(SGMLParser):
    </p>
    <p>
      &#160;&#160;&#160;&#160;def reset(self):&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;
    </p>
    <p>
      &#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;SGMLParser.reset(self)
    </p>
    <p>
      &#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;self.urls = []
    </p>
    <p>
      &#160;&#160;&#160;&#160;def start_a(self, attrs):&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;
    </p>
    <p>
      &#160;&#160;&#160;&#160;&#160;&#160;&#160;&#160;href = [v for k, v in attrs if k=='href']
    </p>
    <p>
